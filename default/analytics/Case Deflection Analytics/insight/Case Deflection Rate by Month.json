{
  "$id": "c_Case_Deflection_Rate",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Case Deflection Rate",
  "description": "Case Deflection Rate by Answers Experience",
  "query": {
    "metrics": [
      "yext/ANSWERS_CASE_DEFLECTION_RATE"
    ],
    "dimensions": [
      "yext/ANSWERS_EXPERIENCE"
    ],
    "filters": [
      {
        "filter": "yext/ANSWERS_CONFIGURATION_VERSION_LABEL",
        "in": [
          "PRODUCTION"
        ]
      },
      {
        "filter": "yext/ANSWERS_QUERY_SOURCE",
        "in": [
          "CASE_DEFLECTION"
        ]
      }
    ]
  },
  "display": {
    "hero": {
      "aggregation": "MEAN"
    }
  }
}
